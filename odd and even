// A voting system that checks if you are eligible to vote.
#include <stdio.h>

int main() { 
    int num1; // num is a library in C hence we can name num as a variable
    printf("Enter number: ");
    scanf("%d", &num1);
    
    (num1 % 2 ==0 ) ? printf("it is even") : printf("it is odd");
    return 0;
}
